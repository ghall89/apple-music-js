{
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended",
    "jxa"
  ],
  "plugins": ["unused-imports"],
  "env": {
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "max-len": [
      "warn",
      {
        "code": 125,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "operator-linebreak": "off",
    "no-console": "error"
  }
}
